{
  "name": "m-transfer",
  "description": "基于 Element UI 的多级多选穿梭框；针对数据量庞大的分页穿梭框",
  "version": "1.0.0",
  "author": "Zhang.qq",
  "private": false,
  "main": "dist/m-transfer.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "npm run serve",
    "build": "vue-cli-service build --progress --hide-modules",
    "lint": "vue-cli-service lint",
    "upload": "node deploy.config.js",
    "deploy": "npm run build && npm run upload"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.12",
    "lodash": "^4.17.21",
    "scp2": "^0.5.0",
    "vue": "2.6.12",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.9",
    "@vue/cli-plugin-eslint": "~4.5.9",
    "@vue/cli-service": "~4.5.9",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "1.32.13",
    "sass-loader": "10.1.1",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
